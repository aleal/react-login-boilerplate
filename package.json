{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate",
  "repository": "git@github.com:aleal/react-boilerplate.git",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development --open --hot",
    "build": "rm -rfv ./prd/build && webpack --mode production",
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "postinstall": "webpack --mode production",
    "start": "node prd/server.js"
  },
  "author": "aleal",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.10.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-md5-hash": "0.0.6"
  }
}
